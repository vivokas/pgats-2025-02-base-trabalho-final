  "main": "index.js",
  "scripts": {
    "test": "mocha 'test/**/*.test.js' --timeout 10000 --reporter mochawesome",
    "test-external": "mocha \"rest/test/external/**/*.test.js\" --timeout 10000 --reporter mochawesome",
    "test-controller": "mocha \"rest/test/controller/**/*.test.js\" --timeout 10000 --reporter mochawesome",
    "test-external-graphql": "mocha \"graphql/test/external/**/*.test.js\" --timeout 10000 --reporter mochawesome",
    "test-all": "mocha \"rest/test/external/**/*.test.js\" \"rest/test/controller/**/*.test.js\" \"graphql/test/external/**/*.test.js\" --timeout 10000 --reporter mochawesome --reporter-options reportDir=mochawesome-report,reportFilename=mochawesome",
    "start": "node rest/server.js",
    "start-graphql": "node graphql/server.js",
    "start-all": "concurrently \"npm run start\" \"npm run start-graphql\""
  },
